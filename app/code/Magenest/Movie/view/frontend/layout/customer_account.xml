<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock class="Magento\Customer\Block\Account\Navigation" name="customer_account_navigation">
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-account-link-custom">
                <arguments>
                    <argument name="path" xsi:type="string">movie/account/dashboard</argument>
                    <argument name="label" xsi:type="string" translate="true">My Dashboard</argument>
                    <argument name="sortOrder" xsi:type="number">300</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
